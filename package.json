{
  "name": "@hcaptcha/vue-hcaptcha",
  "version": "0.3.0",
  "types": "types/index.d.ts",
  "main": "./dist/@hcaptcha/vue-hcaptcha.umd.js",
  "module": "./dist/@hcaptcha/vue-hcaptcha.es.js",
  "exports": {
    ".": {
      "import": "./dist/@hcaptcha/vue-hcaptcha.es.js",
      "require": "./dist/@hcaptcha/vue-hcaptcha.umd.js"
    }
  },
  "files": [
    "dist/@hcaptcha",
    "types/index.d.ts"
  ],
  "author": "hCaptcha team and contributors",
  "homepage": "https://github.com/hCaptcha/vue-hcaptcha",
  "organization": "hCaptcha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hCaptcha/vue-hcaptcha.git",
    "web": "https://github.com/hCaptcha/vue-hcaptcha"
  },
  "bugs": {
    "url": "https://github.com/hCaptcha/vue-hcaptcha/issues"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:types",
    "test:unit": "vue-cli-service test:unit",
    "test:types": "tsc -p types/__tests__",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "build": "vite build",
    "serve": "npm run --prefix examples/traditional-vue-app serve",
    "serve:ts": "npm run --prefix examples/ts-app serve"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-flowtype": "^6.1.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-vue": "^7.18.0",
    "mitt": "^3.0.0",
    "vite": "^2.5.10",
    "vue": "3",
    "vue-demi": "^0.11.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "hcaptcha",
    "captcha",
    "component",
    "vue",
    "vue-component",
    "earn"
  ],
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
